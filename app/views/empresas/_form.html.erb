<%= simple_form_for(@empresa) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nome %>
    <%= f.input :nome_fantasia %>
    <%= f.input :cnpj %>
    <%= f.input :endereco %>
    <%= f.input :bairro %>
    <%= f.input :complemento %>
    <%= f.input :cep %>
    <%= f.input :cidade %>
    <%= f.input :estado %>
    <%= f.input :telefone %>
    <%= f.input :observacao %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
